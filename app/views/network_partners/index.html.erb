<div class="page-header">
  <h1>Listing Network Partners</h1>
</div>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th>Urn</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @network_partners.each do |network_partner| %>
      <tr>
        <td><%= network_partner.name %></td>
        <td><%= network_partner.urn %></td>
        <td><%= link_to 'Show', network_partner %></td>
        <td><%= link_to 'Edit', edit_network_partner_path(network_partner) %></td>
        <td><%= link_to 'Destroy', network_partner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Network partner', new_network_partner_path %>
